# See https://github.com/timrdf/csv2rdf4lod-automation/wiki/conversion:i18n

@prefix conversion: <http://purl.org/twc/vocab/conversion/> .

[] conversion:conversion_process [
   conversion:i18n [
      conversion:interpret [
          conversion:symbol         "source"@en;
          conversion:interpretation "sourcia"@es
      ];
      conversion:interpret [
          conversion:symbol         "dataset"@en;
          conversion:interpretation "dataset"@es;
      ];
      conversion:interpret [
          conversion:symbol         "version"@en;
          conversion:interpretation "version"@es;
      ];
      conversion:interpret [
          conversion:symbol         "conversion"@en;
          conversion:interpretation "conversion"@es;
      ];
      conversion:interpret [
          conversion:symbol         "typed"@en;
          conversion:interpretation "typed"@es;
      ];
      conversion:interpret [
          conversion:symbol         "value-of"@en;
          conversion:interpretation "value-of"@es;
      ];
      conversion:interpret [
          conversion:symbol         "subset/meta"@en;
          conversion:interpretation "subset/meta"@es;
      ];
      conversion:interpret [
          conversion:symbol         "subset/sample"@en;
          conversion:interpretation "subset/sample"@es;
      ];
      conversion:interpret [
          conversion:symbol         "subset/sameas"@en;
          conversion:interpretation "subset/sameas"@es;
      ];
      conversion:interpret [
          conversion:symbol         "provenance"@en;
          conversion:interpretation "provenance"@es;
      ];
      conversion:interpret [
          conversion:symbol         "_page"@en;
          conversion:interpretation "_pagina"@es;
      ];
  ]
] .  
