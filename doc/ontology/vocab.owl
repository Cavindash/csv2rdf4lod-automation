<?xml version="1.0" encoding="utf-8"?>
<rdf:RDF
   xmlns:coin="http://purl.org/court/def/2009/coin#"
   xmlns:conv="http://purl.org/twc/vocab/conversion/"
   xmlns:d2rq="http://www.wiwiss.fu-berlin.de/suhl/bizer/D2RQ/0.1#"
   xmlns:dct="http://purl.org/dc/terms/"
   xmlns:doap="http://usefulinc.com/ns/doap#"
   xmlns:foaf="http://xmlns.com/foaf/0.1/"
   xmlns:ov="http://open.vocab.org/terms/"
   xmlns:owl="http://www.w3.org/2002/07/owl#"
   xmlns:pmlj="http://inference-web.org/2.0/pml-justification.owl#"
   xmlns:pmlp="http://inference-web.org/2.0/pml-provenance.owl#"
   xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
   xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
   xmlns:vann="http://purl.org/vocab/vann/"
   xmlns:void="http://rdfs.org/ns/void#"
   xmlns:vs="http://www.w3.org/2003/06/sw-vocab-status/ns#"
   xmlns:w3test="http://www.w3.org/2006/03/test-description#"
   xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
  <owl:Ontology rdf:about="http://purl.org/twc/vocab/conversion/">
    <dct:creator rdf:resource="http://kasei.us/about/foaf.xrdf#greg"/>
    <dct:creator rdf:resource="http://tw.rpi.edu/instances/TimLebo"/>
    <dct:date>2011-02-09</dct:date>
    <dct:description>Ontology used to describe interpretations of tabular literals to produce RDF.</dct:description>
    <vann:preferredNamespacePrefix>conversion</vann:preferredNamespacePrefix>
    <vann:preferredNamespaceUri>http://purl.org/twc/vocab/conversion/</vann:preferredNamespaceUri>
    <rdfs:comment>This ontology used by the csv2rdf4lod project.</rdfs:comment>
    <rdfs:seeAlso rdf:resource="http://data-gov.tw.rpi.edu/wiki/Csv2rdf4lod"/>
    <rdfs:seeAlso rdf:resource="http://data-gov.tw.rpi.edu/wiki/URI_design_for_RDF_conversion_of_CSV-based_data"/>
    <rdfs:seeAlso rdf:resource="http://doi.acm.org/10.1145/1839707.1839755"/>
    <rdfs:seeAlso rdf:resource="http://prefix.cc/conversion"/>
    <rdfs:seeAlso rdf:resource="http://purl.org/twc/id/software/csv2rdf4lod"/>
    <rdfs:seeAlso rdf:resource="https://github.com/timrdf/csv2rdf4lod-automation/wiki"/>
  </owl:Ontology>
  <rdfs:Class rdf:about="http://purl.org/twc/vocab/conversion/ConversionProcess">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/twc/vocab/conversion/"/>
    <rdfs:label>Superclass for different conversion processes.</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://purl.org/twc/vocab/conversion/DataEndRow">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/twc/vocab/conversion/"/>
    <rdfs:label>DataEndRow</rdfs:label>
    <vs:userdocs rdf:resource="https://github.com/timrdf/csv2rdf4lod-automation/wiki/conversion:DataEndRow"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://purl.org/twc/vocab/conversion/DataStartRow">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/twc/vocab/conversion/"/>
    <rdfs:label>DataStartRow</rdfs:label>
    <vs:userdocs rdf:resource="https://github.com/timrdf/csv2rdf4lod-automation/wiki/conversion:DataStartRow"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://purl.org/twc/vocab/conversion/DocumentState">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/twc/vocab/conversion/"/>
    <rdfs:label>The state of a document at a point in time.</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://purl.org/twc/vocab/conversion/Enhancement">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/twc/vocab/conversion/"/>
    <rdfs:label>The superclass of all enhancements. When the enhancement is referring to a particular property, the property's local name or originating column number may be used (using conversion:property_name and ov:csvCol, respectively).</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
        <owl:onProperty rdf:resource="http://purl.org/twc/vocab/conversion/property_name"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
        <owl:onProperty rdf:resource="http://open.vocab.org/terms/csvCol"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://purl.org/twc/vocab/conversion/EnhancementProcess">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/twc/vocab/conversion/"/>
    <rdfs:label>An enhancement of an RDF dataset that adds new RDF triples</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://purl.org/twc/vocab/conversion/ConversionProcess"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
        <owl:onProperty rdf:resource="http://purl.org/twc/vocab/conversion/enhancement_identifier"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
        <owl:onProperty rdf:resource="http://purl.org/twc/vocab/conversion/enhance"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://purl.org/twc/vocab/conversion/ExistingBundleEnhancement">
    <rdfs:comment>Reasserts the property-value pair to belong to the existing resource specified by the conversion:bundled_by property.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/twc/vocab/conversion/"/>
    <rdfs:label>Existing bundle enhancement</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://purl.org/twc/vocab/conversion/SubjectEnhancement"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
        <owl:onProperty rdf:resource="http://purl.org/twc/vocab/conversion/bundled_by"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="http://purl.org/twc/vocab/conversion/ResourcePromotionEnhancement"/>
        <owl:onProperty rdf:resource="http://purl.org/twc/vocab/conversion/bundled_by"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://purl.org/twc/vocab/conversion/HeaderRow">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/twc/vocab/conversion/"/>
    <rdfs:label>HeaderRow</rdfs:label>
    <vs:userdocs rdf:resource="https://github.com/timrdf/csv2rdf4lod-automation/wiki/conversion:HeaderRow"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://purl.org/twc/vocab/conversion/ImplicitBundle">
    <rdfs:comment>Represents the structural template for an implicit bundle enhancement. The conversion:property_name specifies the property name by which a bundle is attached to an instance. An optional conversion:type_name may be specified for the bundle to indicate the rdf:type of the new resource.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/twc/vocab/conversion/"/>
    <rdfs:label>Implicit Bundle</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
        <owl:onProperty rdf:resource="http://purl.org/twc/vocab/conversion/property_name"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
        <owl:onProperty rdf:resource="http://purl.org/twc/vocab/conversion/type_name"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:disjointWith rdf:resource="http://purl.org/twc/vocab/conversion/ImplicitBundleEnhancement"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://purl.org/twc/vocab/conversion/ImplicitBundleEnhancement">
    <rdfs:comment>Reasserts the property-value pair to belong to a new resource specified by the conversion:bundled_by property. An example of this enhancement would be the introduction of a geo:Point instance based on latitude and longitude properties.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/twc/vocab/conversion/"/>
    <rdfs:label>Implicit bundle enhancement</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://purl.org/twc/vocab/conversion/SubjectEnhancement"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
        <owl:onProperty rdf:resource="http://purl.org/twc/vocab/conversion/bundled_by"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://purl.org/twc/vocab/conversion/bundled_by"/>
        <owl:someValuesFrom rdf:resource="http://purl.org/twc/vocab/conversion/ImplicitBundle"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://purl.org/twc/vocab/conversion/ObjectEnhancement">
    <rdfs:comment>Enhancement modifies the object of the resulting triple.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/twc/vocab/conversion/"/>
    <rdfs:label>ObjectEnhancement</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://purl.org/twc/vocab/conversion/Enhancement"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://purl.org/twc/vocab/conversion/Omitted">
    <rdfs:comment>Omit all triples from this column.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/twc/vocab/conversion/"/>
    <rdfs:subClassOf rdf:resource="http://purl.org/twc/vocab/conversion/Enhancement"/>
    <vs:userdocs rdf:resource="https://github.com/timrdf/csv2rdf4lod-automation/wiki/conversion:Omitted"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://purl.org/twc/vocab/conversion/Only_if_column">
    <rdfs:comment>Omit triple if there is no value in the cell of the given column.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/twc/vocab/conversion/"/>
    <rdfs:seeAlso rdf:resource="http://www.wiwiss.fu-berlin.de/suhl/bizer/D2RQ/0.1#condition"/>
    <rdfs:seeAlso rdf:resource="https://github.com/timrdf/csv2rdf4lod-automation/wiki/conversion:Only_if_column"/>
    <rdfs:subClassOf rdf:resource="http://purl.org/twc/vocab/conversion/Enhancement"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://purl.org/twc/vocab/conversion/PredicateEnhancement">
    <rdfs:comment>Enhancement modifies the predicate of the resulting triple.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/twc/vocab/conversion/"/>
    <rdfs:label>PredicateEnhancement</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://purl.org/twc/vocab/conversion/Enhancement"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://purl.org/twc/vocab/conversion/PrimaryKeyEnhancement">
    <rdfs:comment>Uses the property identified by name with conversion:primary_key in generating instance URIs. For example, a conversion:primary_key of 'user_id' would result in a URI like &lt;/source/SSS/dataset/DDD/version/VVV/user_id/UUU&gt; instead of &lt;/source/SSS/dataset/DDD/version/VVV/thing_1&gt;.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/twc/vocab/conversion/"/>
    <rdfs:label>Primary key enhancement</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://purl.org/twc/vocab/conversion/SubjectEnhancement"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://purl.org/twc/vocab/conversion/RawConversionProcess">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/twc/vocab/conversion/"/>
    <rdfs:label>A raw conversion process from CSV to RDF</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://purl.org/twc/vocab/conversion/ConversionProcess"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">0</owl:maxCardinality>
        <owl:onProperty rdf:resource="http://purl.org/twc/vocab/conversion/enhancement_identifier"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://purl.org/twc/vocab/conversion/SubjectEnhancement">
    <rdfs:comment>Enhancement modifies the subject of the resulting triple.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/twc/vocab/conversion/"/>
    <rdfs:label>SubjectEnhancement</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://purl.org/twc/vocab/conversion/Enhancement"/>
  </rdfs:Class>
  <rdf:Property rdf:about="http://purl.org/twc/vocab/conversion/bundled_by">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/twc/vocab/conversion/"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
    <vs:userdocs rdf:resource="https://github.com/timrdf/csv2rdf4lod-automation/wiki/conversion:bundled_by"/>
  </rdf:Property>
  <rdf:Property rdf:about="http://purl.org/twc/vocab/conversion/delimits_cell">
    <rdfs:comment>The delimiter that should be used to separate cells in a row.</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/twc/vocab/conversion/ConversionProcess"/>
    <rdfs:label>delimits_cell</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <vs:userdocs rdf:resource="https://github.com/timrdf/csv2rdf4lod-automation/wiki/conversion:delimits_cell"/>
  </rdf:Property>
  <rdf:Property rdf:about="http://purl.org/twc/vocab/conversion/delimits_object">
    <rdfs:comment>A regular expression specifiying how the object literal should be delimited to produce multiple object values.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/twc/vocab/conversion/"/>
    <rdfs:label>delimits_object</rdfs:label>
    <vs:userdocs rdf:resource="https://github.com/timrdf/csv2rdf4lod-automation/wiki/conversion:delimits_object"/>
  </rdf:Property>
  <rdf:Property rdf:about="http://purl.org/twc/vocab/conversion/domain_template">
    <rdfs:comment>TODO, cannot happen: :cp a conversion:EnhancementConversionProcess; conversion:enhance [ ov:csvCol 1; conversion:domain_template </rdfs:comment>
    <rdfs:comment>Template to name the subject of the triple produced.</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/twc/vocab/conversion/SubjectEnhancement"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/twc/vocab/conversion/"/>
    <vs:userdocs rdf:resource="https://github.com/timrdf/csv2rdf4lod-automation/wiki/conversion:domain_template"/>
  </rdf:Property>
</rdf:RDF>
